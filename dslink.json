{
  "name": "template-dslink-java",
  "version": "1.0",
  "description": "An example template for Java DSLinks",
  "license": "Apache",
  "author": {
    "name": "Logan Gorence",
    "email": "loganjohngorence@gmail.com"
  },
  "main": "bin/template-dslink-java",
  "repository": {
    "type": "git",
    "url": "https://github.com/IOT-DSA/template-dslink-java"
  },
  "bugs": {
    "url": "https://github.com/IOT-DSA/template-dslink-java/issues"
  },
  "configs": {
    "broker": {
